    "homepage": "https://github.com/ducascao/api-maker",
    "keywords": ["Laravel", "ApiMaker"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ducascao\\ApiMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ducascao\\ApiMaker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ducascao\\ApiMaker\\ApiMakerServiceProvider"
            ],
            "aliases": {
                "ApiMaker": "Ducascao\\ApiMaker\\Facades\\ApiMaker"
            }
        }
    }
}
